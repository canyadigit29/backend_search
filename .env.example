# CORS: allow the Next.js app in dev
ALLOWED_ORIGINS=http://127.0.0.1:3100,http://localhost:3100

# Supabase Storage bucket used by upload/OCR endpoints
SUPABASE_STORAGE_BUCKET=files

# Logging
LOG_LEVEL=INFO
LOG_JSON=false

